        function setupAutoBackup() {
            const lastBackup = localStorage.getItem('lastBackup');
            const autoBackup = localStorage.getItem('autoBackup') || '7';
            
            if (autoBackup !== '0' && lastBackup) {
                const daysSinceLastBackup = Math.floor((new Date() - new Date(lastBackup)) / (1000 * 60 * 60 * 24));
                
                if (daysSinceLastBackup >= parseInt(autoBackup)) {
                    createAutoBackup();
                }
            }
        }

        function createAutoBackup() {
            const autoBackup = localStorage.getItem('autoBackup') || '7';
            
            if (autoBackup !== '0') {
                const backupData = {
                    keys: keys,
                    timestamp: new Date().toISOString(),
                    version: '1.0'
                };
                
                localStorage.setItem('starlinkBackup', JSON.stringify(backupData));
                localStorage.setItem('lastBackup', new Date().toISOString());
                
                console.log('‚úÖ Backup autom√°tico criado');
            }
        }

        // Inicializar sistema quando a p√°gina carregar
        document.addEventListener('DOMContentLoaded', initializeAdminSystem);
    </script>
